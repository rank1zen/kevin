package component

import "context"

type MenuItemLayout struct {
	Label string

	SubLabel string
}

templ (m MenuItemLayout) ToTempl(ctx context.Context) {
	<div class="flex flex-item h-7 items-center justify-between px-3 gap-3">
		<span class="text-sm text-gray-900/90 whitespace-nowrap dark:text-gray-100/90">{ m.Label }</span>
		<span class="text-sm text-gray-500/90 whitespace-nowrap">{ m.SubLabel }</span>
	</div>
}
