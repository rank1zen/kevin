	OpenMenuIcon    = buildIcon("open-menu")
)

func buildIcon(name string) Icon {
	templComp := templ.ComponentFunc(
		func(ctx context.Context, w io.Writer) error {
			_, err := w.Write([]byte(iconSVGData[name]))
			return err
		},
	)

	a := func(ctx context.Context) templ.Component {
		return templComp
	}

	return ComponentFunc(a)
}

